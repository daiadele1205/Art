@model IEnumerable<ArtworkPortfolio>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

@if (Model.Count() > 0)
{

        @foreach (var item in Model)
        {
                <div class="row">
                        <img class='resize_fit_center' style="height: 460px; width: 100%; object-fit: cover !important;" src="@item.Image"/>
                </div>
        }
        
}

